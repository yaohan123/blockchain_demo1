<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yaohan.postlike.mapper.UserLikeMapper">
    <resultMap id="UserLikeResult" type="com.yaohan.postlike.domain.UserLike" >
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="liked_user_id" property="likedUserId" jdbcType="VARCHAR" />
        <result column="liked_post_id" property="likedPostId" jdbcType="VARCHAR" />
        <result column="status" property="status" jdbcType="INTEGER" />
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
        <result column="update_time" property="updateTime" jdbcType="TIMESTAMP" />
    </resultMap>

    <select id="selectUserLikeById" parameterType="java.lang.Integer" resultMap="UserLikeResult">
        SELECT * FROM user_like WHERE id = #{id,jdbcType=INTEGER};
    </select>
